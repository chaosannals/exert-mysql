{
    "name": "chaosannals/exert-mysql",
    "description": "mysql exert",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "chaosannals",
            "email": "chaosannals@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "exert\\": "main"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\": "test"
        }
    },
    "require": {
        "doctrine/orm": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "scripts": {
        "create": "doctrine orm:schema-tool:create",
        "drop": "doctrine orm:schema-tool:drop --force",
        "update": "doctrine orm:schema-tool:update --force",
        "dump-update": "doctrine orm:schema-tool:update --dump-sql"
    }
}